// String processing operations
string -> create("Calculation result: ")
string -> append(calculator_result)
string -> format_number(2)

// Output operations (irreversible)
display -> clear_screen()
display -> print_header("L5 Moop Advanced Demo")
display -> print_message(final_string)
display -> print_footer()

// Final output
output -> "L5 Moop advanced compilation completed!"
